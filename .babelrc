{
  "env": {
    "development": {
      "plugins": [ "transform-es2015-modules-commonjs" ]
    },
    "production": {
      "presets": [ "es2015" ]
    }
  },
  "plugins": [
    [ "transform-async-to-module-method", {
      "module": "bluebird",
      "method": "coroutine"
    } ],
    "transform-class-properties"
  ]
}
